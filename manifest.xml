<package version="0.0">
  <name>org.openxtalk.library.libsvgicons</name>
  <title>OpenXTalk libSVGIcons</title>
  <author>OpenXTalk</author>
  <version>0.1.0</version>
  <license>community</license>
  <type>library</type>
  <requires name="com.livecode.foreign"/>
  <requires name="com.livecode.library.iconsvg"/>
  <requires name="com.livecode.canvas"/>
  <requires name="com.livecode.arithmetic"/>
  <requires name="com.livecode.binary"/>
  <requires name="com.livecode.list"/>
  <requires name="com.livecode.logic"/>
  <requires name="com.livecode.string"/>
  <requires name="com.livecode.type"/>
  <requires name="com.livecode.engine"/>
  <requires name="com.livecode.file"/>
  <requires name="com.livecode.typeconvert"/>
  <requires name="com.livecode.char"/>
  <requires name="com.livecode.array"/>
  <requires name="com.livecode.byte"/>
  <metadata key="_ide">true</metadata>
  <handler name="DataFromUTF8" parameters="in string" return="data"/>
  <handler name="DataToUTF8" parameters="in data" return="string"/>
  <handler name="loadOXTIcons" parameters="" return="nothing"/>
  <handler name="loadAllIconSets" parameters="" return="nothing"/>
  <handler name="loadIconSetFromTSVFile" parameters="in string,in string" return="nothing"/>
  <handler name="verticalFlippedOfSVGPath" parameters="in any" return="string"/>
  <handler name="horizontalFlippedOfSVGPath" parameters="in any" return="string"/>
  <handler name="horizScaledOfSVGPath" parameters="in integer,in any" return="string"/>
  <handler name="vertScaledOfSVGPath" parameters="in integer,in any" return="string"/>
</package>